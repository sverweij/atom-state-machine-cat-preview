{
  "atom-workspace atom-text-editor:not(.mini)[data-grammar='source smcat']": {
    "ctrl-shift-g": "state-machine-cat-preview:toggle",
    "ctrl-shift-v": "state-machine-cat-preview:toggle"
  },
  ".platform-darwin .state-machine-cat-preview": {
    "cmd-+": "state-machine-cat-preview:zoom-in",
    "cmd-=": "state-machine-cat-preview:zoom-in",
    "cmd--": "state-machine-cat-preview:zoom-out",
    "cmd-_": "state-machine-cat-preview:zoom-out",
    "cmd-0": "state-machine-cat-preview:reset-zoom",
    "cmd-9": "state-machine-cat-preview:zoom-to-fit"
  },
  ".platform-win32 .state-machine-cat-preview, .platform-linux .state-machine-cat-preview": {
    "ctrl-+": "state-machine-cat-preview:zoom-in",
    "ctrl-=": "state-machine-cat-preview:zoom-in",
    "ctrl--": "state-machine-cat-preview:zoom-out",
    "ctrl-_": "state-machine-cat-preview:zoom-out",
    "ctrl-0": "state-machine-cat-preview:reset-zoom",
    "ctrl-9": "state-machine-cat-preview:zoom-to-fit"
  }
}
